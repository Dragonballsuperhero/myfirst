{"ast":null,"code":"import Nav from '@/components/Nav.vue';\nimport RoleManger from '@/views/power/RoleManger.vue';\nexport default {\n  name: 'Roles',\n  components: {\n    Nav,\n    RoleManger\n  },\n  data() {\n    return {\n      rolesList: [],\n      //所有的角色列表\n      addDialogVisible: false,\n      //控制添加角色对话框是否显示\n      editDialogVisible: false,\n      //控制编辑角色对话框是否显示\n      treeDialogVisible: false,\n      //控制树形权限结构对话框是否显示\n      addRolesForm: {\n        //添加表单进行双向绑定\n        roleName: '',\n        roleDesc: ''\n      },\n      editRolesForm: {},\n      //编辑表单的数据\n      treeRightList: [],\n      //树形控件绑定的列表\n      treeRightField: {\n        //树形控件绑定的属性\n        children: 'children',\n        label: 'authName'\n      },\n      rolesRules: {\n        //添加角色表单的验证规则\n        roleName: [{\n          required: true,\n          message: '请输入角色名称',\n          trigger: 'blur'\n        }, {\n          min: 2,\n          max: 10,\n          message: '长度在 2 到 10 个字符',\n          trigger: 'blur'\n        }],\n        roleDesc: [{\n          required: true,\n          message: '角色描述',\n          trigger: 'blur'\n        }, {\n          min: 5,\n          max: 15,\n          message: '长度在 5 到 15 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.getRolesList();\n  },\n  methods: {\n    // 获取所有的角色\n    async getRolesList() {\n      const {\n        data: res\n      } = await this.$http.get('roles');\n      this.notification(res, '请求角色列表失败', '请求角色列表成功', 200);\n      this.rolesList = res.data;\n    },\n    // 当对话框关闭后清空表单的所有数据\n    addCloseDialog() {\n      this.$refs.addFormRef.resetFields();\n    },\n    editCloseDialog() {\n      this.$refs.editFormRef.resetFields();\n    },\n    // 发起请求添加角色\n    addRoles() {\n      //预验证\n      this.$refs.addFormRef.validate(async vali => {\n        if (!vali) return;\n        const {\n          data: res\n        } = await this.$http.post('roles', this.addRolesForm);\n        this.notification(res, '创建角色失败', '创建角色成功', 201);\n        this.getRolesList();\n        this.addDialogVisible = false;\n      });\n    },\n    //根据用户的id拿到对应的数据\n    async editRolesById(id) {\n      const {\n        data: res\n      } = await this.$http.get('roles/' + id);\n      // 获取到数据存入editRolesForm\n      this.editRolesForm = res.data;\n      this.editDialogVisible = true;\n    },\n    //编辑角色\n    editRoles() {\n      this.$refs.editFormRef.validate(async vali => {\n        if (!vali) return;\n        const {\n          data: res\n        } = await this.$http.put('roles/' + this.editRolesForm.roleId, {\n          roleName: this.editRolesForm.roleName,\n          roleDesc: this.editRolesForm.roleDesc\n        });\n        this.notification(res, '编辑角色失败', '编辑角色成功', 200);\n        this.getRolesList();\n        this.editDialogVisible = false;\n        console.log(res);\n      });\n    },\n    // 删除用户信息\n    async removeRoleInfo(id) {\n      const result = await this.$confirm('此操作将永久删除该角色, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        center: true\n      }).catch(err => err);\n      // 用户点击取消按钮返回的是cancel\n      // 用户点击确认按钮返回的是confirm\n      if (result === 'cancel') {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      } else {\n        //确认删除\n        const {\n          data: res\n        } = await this.$http.delete('roles/' + id);\n        this.notification(res, '删除角色失败', '删除角色成功', 200);\n        this.getRolesList();\n      }\n    },\n    //获取tree权限列表\n    async getTreeRightList() {\n      this.treeDialogVisible = true;\n      const {\n        data: res\n      } = await this.$http.get('rights/tree');\n      this.treeRightList = res.data;\n      console.log(this.treeRightField);\n    }\n  }\n};","map":{"version":3,"mappings":"AA8FA;AACA;AACA;EACAA;EACAC;IAAAC;IAAAC;EAAA;EACAC;IACA;MACAC;MAAA;MACAC;MAAA;MACAC;MAAA;MACAC;MAAA;MACAC;QAAA;QACAC;QACAC;MACA;MACAC;MAAA;MACAC;MAAA;MACAC;QAAA;QACAC;QACAC;MACA;MACAC;QAAA;QACAP,WACA;UAAAQ;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,EACA;QACAT,WACA;UAAAO;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA;MAEA;IACA;EACA;EAEAG;IACA;EACA;EACAC;IACA;IACA;MACA;QAAApB;MAAA;MACA;MACA;IACA;IACA;IACAqB;MACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA;MACA;QACA;QACA;UAAAvB;QAAA;QACA;QACA;QACA;MACA;IACA;IACA;IACA;MACA;QAAAA;MAAA;MACA;MACA;MACA;IACA;IACA;IACAwB;MACA;QACA;QACA;UAAAxB;QAAA,+DACA;UAAAM;UAAAC;QAAA;QACA;QACA;QACA;QACAkB;MACA;IACA;IACA;IACA;MACA;QACAC;QACAC;QACAC;QACAC;MACA;MACA;MACA;MACA;QACA;UACAD;UACAb;QACA;MACA;QACA;QACA;UAAAf;QAAA;QACA;QACA;MACA;IACA;IACA;IACA;MACA;MACA;QAAAA;MAAA;MACA;MACAyB;IACA;EACA;AACA","names":["name","components","Nav","RoleManger","data","rolesList","addDialogVisible","editDialogVisible","treeDialogVisible","addRolesForm","roleName","roleDesc","editRolesForm","treeRightList","treeRightField","children","label","rolesRules","required","message","trigger","min","max","created","methods","addCloseDialog","editCloseDialog","addRoles","editRoles","console","confirmButtonText","cancelButtonText","type","center"],"sourceRoot":"src/views/power","sources":["Roles.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 导航区域 -->\r\n    <Nav>\r\n      <template #homePage>首页</template>\r\n      <template #context1>权限管理</template>\r\n      <template #context2>角色列表</template>\r\n    </Nav>\r\n    <!-- 卡片区域 -->\r\n    <el-card>\r\n      <el-row>\r\n        <el-col>\r\n          <!-- 添加角色按钮 -->\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\">添加角色</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 表格 -->\r\n      <el-table :data=\"rolesList\" border stripe>\r\n            <!-- 下拉键 -->\r\n            <el-table-column label=\"#\" type=\"expand\">\r\n              <template slot-scope=\"scope\">\r\n                  <!-- 下拉内容在RoleManger组件 -->\r\n              <RoleManger :roles=\"scope.row\"></RoleManger>\r\n              </template>\r\n            </el-table-column>\r\n            <!-- 索引键 -->\r\n            <el-table-column label=\"#\" type=\"index\"></el-table-column>\r\n            <el-table-column label=\"角色名称\" prop=\"roleName\"></el-table-column>\r\n            <el-table-column label=\"角色描述\" prop=\"roleDesc\"></el-table-column>\r\n            <el-table-column label=\"操作\">\r\n              <!-- 操作按钮 -->\r\n              <template scope=\"scope\">\r\n                <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\" @click=\"editRolesById(scope.row.id)\">编辑</el-button>\r\n                <el-button type=\"danger\"  size=\"mini\"  icon=\"el-icon-delete\" @click=\"removeRoleInfo(scope.row.id)\">删除</el-button>\r\n                <el-button type=\"warning\" size=\"mini\" icon=\"el-icon-setting\" @click=\"getTreeRightList(scope.row)\">分配角色</el-button>\r\n              </template>\r\n            </el-table-column>\r\n        </el-table>\r\n    </el-card>\r\n    <!-- 添加角色对话框 -->\r\n    <el-dialog\r\n        title=\"添加角色\" :visible.sync=\"addDialogVisible\" width=\"45%\"\r\n        @close=\"addCloseDialog\">\r\n        <!-- 表单 -->\r\n        <el-form status-icon :model=\"addRolesForm\" :rules=\"rolesRules\" ref=\"addFormRef\" label-width=\"100px\">\r\n      <el-form-item label=\"角色名称\" prop=\"roleName\">\r\n          <el-input v-model=\"addRolesForm.roleName\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"角色描述\" prop=\"roleDesc\">\r\n          <el-input v-model=\"addRolesForm.roleDesc\"></el-input>\r\n      </el-form-item>\r\n      </el-form>\r\n      <!-- 按钮 -->\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"addRoles\">确 定</el-button>\r\n  </span>\r\n</el-dialog>\r\n\r\n<!-- 编辑角色对话框 -->\r\n<el-dialog\r\n        title=\"添加角色\" :visible.sync=\"editDialogVisible\" width=\"45%\"\r\n        @close=\"editCloseDialog\">\r\n        <!-- 表单 -->\r\n        <el-form status-icon :model=\"editRolesForm\" :rules=\"rolesRules\" ref=\"editFormRef\" label-width=\"100px\">\r\n      <el-form-item  label=\"角色名称\" prop=\"roleName\">\r\n          <el-input disabled v-model=\"editRolesForm.roleName\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"角色描述\" prop=\"roleDesc\">\r\n          <el-input v-model=\"editRolesForm.roleDesc\"></el-input>\r\n      </el-form-item>\r\n      </el-form>\r\n      <!-- 按钮 -->\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"editDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"editRoles\">确 定</el-button>\r\n  </span>\r\n</el-dialog>\r\n<!-- 树形权限结构 -->\r\n<el-dialog\r\n        title=\"添加权限\" :visible.sync=\"treeDialogVisible\" width=\"45%\">\r\n        <!-- 树形控件 -->\r\n        <el-tree :data=\"treeRightList\" :props=\"treeRightField\" show-checkbox \r\n        node-key=\"id\" default-expand-all></el-tree>\r\n      <!-- 按钮 -->\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"treeDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\">确 定</el-button>\r\n  </span>\r\n</el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Nav from '@/components/Nav.vue'\r\nimport RoleManger from '@/views/power/RoleManger.vue'\r\nexport default {\r\n  name:'Roles',\r\n  components:{Nav,RoleManger},\r\n  data() {\r\n    return {\r\n      rolesList: [],//所有的角色列表\r\n      addDialogVisible:false,//控制添加角色对话框是否显示\r\n      editDialogVisible:false,//控制编辑角色对话框是否显示\r\n      treeDialogVisible: false,//控制树形权限结构对话框是否显示\r\n      addRolesForm:{   //添加表单进行双向绑定\r\n        roleName:'',\r\n        roleDesc:''\r\n      },\r\n      editRolesForm:{},//编辑表单的数据\r\n      treeRightList:[],//树形控件绑定的列表\r\n      treeRightField:{ //树形控件绑定的属性\r\n        children:'children',\r\n        label:'authName'\r\n      },\r\n      rolesRules:{ //添加角色表单的验证规则\r\n        roleName:[\r\n        { required: true, message: '请输入角色名称', trigger: 'blur' },\r\n        { min: 2, max: 10, message: '长度在 2 到 10 个字符', trigger: 'blur' }\r\n        ],\r\n        roleDesc:[\r\n        { required: true, message: '角色描述', trigger: 'blur' },\r\n        { min: 5, max: 15, message: '长度在 5 到 15 个字符', trigger: 'blur' }\r\n        ],   \r\n      }\r\n    };\r\n  },\r\n  \r\n  created(){\r\n    this.getRolesList();\r\n  },\r\n  methods: {\r\n    // 获取所有的角色\r\n   async getRolesList() {\r\n      const {data:res} = await this.$http.get('roles');\r\n      this.notification(res,'请求角色列表失败','请求角色列表成功',200);\r\n      this.rolesList = res.data;\r\n    },\r\n    // 当对话框关闭后清空表单的所有数据\r\n    addCloseDialog(){\r\n      this.$refs.addFormRef.resetFields();\r\n    },\r\n    editCloseDialog(){\r\n      this.$refs.editFormRef.resetFields();\r\n    },\r\n    // 发起请求添加角色\r\n    addRoles(){\r\n      //预验证\r\n      this.$refs.addFormRef.validate(async vali=>{\r\n        if(!vali)return;\r\n        const {data:res} = await this.$http.post('roles',this.addRolesForm)\r\n        this.notification(res,'创建角色失败','创建角色成功',201);\r\n        this.getRolesList();\r\n        this.addDialogVisible = false;\r\n      });\r\n    },\r\n    //根据用户的id拿到对应的数据\r\n   async editRolesById(id){\r\n     const {data:res} = await this.$http.get('roles/'+id);\r\n      // 获取到数据存入editRolesForm\r\n     this.editRolesForm =  res.data;\r\n     this.editDialogVisible = true;\r\n    },\r\n    //编辑角色\r\n    editRoles(){\r\n      this.$refs.editFormRef.validate(async vali=>{\r\n        if(!vali)return;\r\n        const {data:res} = await this.$http.put('roles/'+this.editRolesForm.roleId,\r\n        {roleName:this.editRolesForm.roleName,roleDesc:this.editRolesForm.roleDesc});\r\n        this.notification(res,'编辑角色失败','编辑角色成功',200);\r\n        this.getRolesList();\r\n        this.editDialogVisible = false;\r\n        console.log(res);\r\n      });\r\n    },\r\n      // 删除用户信息\r\n async removeRoleInfo(id){\r\n   const result = await this.$confirm('此操作将永久删除该角色, 是否继续?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning',\r\n          center: true\r\n        }).catch(err=>err);\r\n        // 用户点击取消按钮返回的是cancel\r\n        // 用户点击确认按钮返回的是confirm\r\n      if(result==='cancel'){\r\n        this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n      }else{\r\n        //确认删除\r\n        const {data:res} =  await this.$http.delete('roles/'+id);\r\n        this.notification(res,'删除角色失败','删除角色成功',200);\r\n        this.getRolesList();\r\n      }\r\n    },\r\n    //获取tree权限列表\r\n   async getTreeRightList(){\r\n      this.treeDialogVisible = true;\r\n      const {data:res} = await this.$http.get('rights/tree');\r\n      this.treeRightList =  res.data;\r\n      console.log(this.treeRightField);\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}